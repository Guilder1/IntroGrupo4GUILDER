Algoritmo orden_inverso
	dimension m[10]
	Para  i <-1 Hasta 10 Con Paso 1 Hacer
		Leer m[i]
	Fin Para
	Para  i <-1 Hasta 10 Con Paso 1 Hacer
		Mostrar m[i] Sin Saltar " "
	Fin Para
	Escribir " "
	escribir" El orden inverso de los numeros ingresados es "
	Para  i <- 10 Hasta 1 Con Paso -1 Hacer
		Mostrar m[i] Sin Saltar " "
	Fin Para
	escribir " "
FinAlgoritmo